{
  "name": "iui-wizard",
  "version": "1.0.0",
  "description": "Angular directive & styles",
  "main": "./lib/index.js",
  "private": false,
  "scripts": {
    "start": "npm run server",
    "build": "gulp build",
    "test": "gulp test",
    "test-continuous": "gulp testContinuous",
    "server": "gulp server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medseek-engineering/iui-wizard.git"
  },
  "author": "Influence Health, Inc.",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/medseek-engineering/iui-wizard/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "angular": "~1.3.2",
    "angular-mocks": "~1.3.2",
    "babel": "^6.5.2",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.8.2",
    "delayed-stream": "0.0.5",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "~2.1.7",
    "gulp-filter": "^3.0.0",
    "gulp-hologram": "^1.2.0",
    "gulp-html2js": "^0.2.0",
    "gulp-jshint": "^1.11.2",
    "gulp-minify-css": "^1.2.0",
    "gulp-ng-html2js": "^0.2.0",
    "gulp-rename": "~0.2.2",
    "gulp-uglify": "~0.2.0",
    "gulp-util": "~2.2.12",
    "iui-general": "^1.0.0",
    "jquery": "~2.1.4",
    "jshint": "~2.3.0",
    "jshint-stylish": "~1.0.0",
    "karma": "~0.12.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "~0.1",
    "karma-html-reporter": "~0.1.1",
    "karma-jasmine": "~0.2.2",
    "karma-ng-html2js-preprocessor": "~0.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-requirejs": "~0.2.0",
    "karma-spec-reporter": "0.0.6",
    "phantomjs-prebuilt": "^2.1.4"
  },
  "buildSettings": {
    "base": "./app/",
    "styleModules": [
      "iui-general"
    ],
    "createTemplates": {
      "moduleName": "iui.wizardTemplates",
      "prefix": "/$iui-wizard/",
      "templateFile": "templates.js"
    },
    "combineFiles": {
      "ignore": "!./lib/src/**/*.test.js"
    },
    "compass": {
      "sass": "lib/src",
      "css": "lib/dist",
      "require": []
    },
    "appFiles": {
      "styles": [
        "./lib/src/**/*.scss",
        "./lib/src/*.scss"
      ],
      "htmlFiles": [
        "./lib/src/**/*.html",
        "./lib/src/**/*.js"
      ],
      "jsFiles": [
        "./lib/src/**/*.js",
        "./lib/src/*.js"
      ],
      "templateFiles": [
        "./lib/src/**/*.html"
      ]
    },
    "destination": {
      "css": "./lib/dist",
      "js": "./lib/dist",
      "jsFile": "iui-module.js",
      "jsFileMin": "iui-module.min.js"
    }
  }
}
